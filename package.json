  },
  "homepage": "https://github.com/scottdj92/next-artemis#readme",
  "devDependencies": {
    "@types/isomorphic-fetch": "0.0.34",
    "@types/jest": "^23.1.1",
    "@types/next": "^6.0.1",
    "@types/react": "^16.4.1",
    "@types/react-dom": "^16.0.6",
    "apollo-test-utils": "^0.3.2",
    "enzyme": "^3.3.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.0.2",
    "jest": "^23.1.0",
